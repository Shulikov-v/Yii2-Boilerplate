{
    "name": "combc/yii2boilerplate",
    "description": "Structure for enterprise-grade websites for Yii2 framework.",
    "keywords": ["yii2", "framework", "boilerplate", "vagrant"],
    "homepage": "http://www.combc.com/",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/combc/Yii2Boilerplate/issues?state=open",
        "source": "https://github.com/combc/Yii2Boilerplate"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "behat/mink": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
